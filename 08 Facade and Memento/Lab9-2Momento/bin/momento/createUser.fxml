                        <Font size="14.0" />
                    </font>
                </TextField>
                <TextField fx:id="userEmail" layoutX="103.0" layoutY="94.0" onKeyTyped="#textFieldEvent" prefHeight="25.0" prefWidth="330.0" promptText="Email Address">
                    <font>
                        <Font size="14.0" />
                    </font>
                </TextField>
                <TextField fx:id="userContact" layoutX="103.0" layoutY="132.0" onKeyTyped="#textFieldEvent" prefHeight="25.0" prefWidth="330.0" promptText="Contact Number">
